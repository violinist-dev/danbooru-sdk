{
  "name": "desu-project/danbooru-sdk",
  "description": "PHP SDK for Danbooru.",
  "type": "library",
  "keywords": [
    "sdk",
    "danbooru",
    "implements-chanbooru"
  ],
  "homepage": "https://github.com/desu-project/danbooru-sdk",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/desu-project/danbooru-sdk/issues",
    "source": "https://github.com/desu-project/danbooru-sdk"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.11"
  },
  "authors": [
    {
      "name": "Petr Flaks",
      "email": "lithx@yandex.com"
    }
  ],
  "require": {
    "php" : "^7.2",
    "desu-project/chanbooru-interface": "^0.0.0"
  },
  "autoload": {
    "psr-4": {
      "DesuProject\\DanbooruSdk\\": "src/"
    }
  },
  "scripts": {
    "fix-cs": [
      "vendor/bin/php-cs-fixer fix --config=.php_cs.dist --verbose --show-progress=dots"
    ],
    "lint": [
      "vendor/bin/php-cs-fixer fix --config=.php_cs.dist --verbose --show-progress=dots --dry-run"
    ]
  }
}
